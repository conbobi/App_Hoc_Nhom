{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
   
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true, // Thêm dòng này để bỏ qua kiểm tra kiểu trong thư viện
    "resolveJsonModule": true,
    "jsx": "react-native",

    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
